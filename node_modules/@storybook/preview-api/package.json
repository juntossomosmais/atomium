    "access": "public"
  },
  "bundler": {
    "entries": [
      "./src/index.ts",
      "./src/addons.ts",
      "./src/client-api.ts",
      "./src/core-client.ts",
      "./src/preview-web.ts",
      "./src/store.ts"
    ]
  },
