{
  "name": "@juntossomosmais/atomium-tokens",
  "version": "0.1.0-alpha.18",
  "description": "Tokens and variables for Atomium",
  "access": "public",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/juntossomosmais/atomium.git",
    "directory": "packages/tokens"
  },
  "publishConfig": {
    "@juntossomosmais:registry": "https://npm.pkg.github.com",
    "access": "public"
  },
  "exports": {
    "./*": "./dist/*",
    ".": "./dist/index.js"
  },
  "scripts": {
    "start": "rollup -c -w",
    "convert-tokens": "rollup ./index.ts --sourcemap true -o dist/index.js -f cjs",
    "build": "rollup -c && tsc && node dist/scripts/generate-tokens.js && npm run convert-tokens",
    "publish-library": "npm publish --access public"
  },
  "files": [
    "dist"
  ]
}
